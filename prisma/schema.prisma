generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  createdat DateTime @default(now())
}

model Spares {
  id             Int     @id @default(autoincrement())
  partname       String
  partcode       String
  customername   String
  customernumber String
  warrantystatus String
  company        String
  partsent       Boolean @default(false)
  recevingdate   String
}
